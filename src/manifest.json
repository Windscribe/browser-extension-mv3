{
  "manifest_version": 3,
  "version": "1.0.10",
  "minimum_chrome_version": "105.0",
  "name": "Windscribe - Experimental MV3",
  "description": "Windscribe MV3 is an experimental extension that masks your physical location, circumvents censorship, and block ads and trackers",
  "background": { "service_worker": "backgroundScripts.js", "type": "module" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "permissions": [
    "activeTab",
    "contentSettings",
    "contextMenus",
    "debugger",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "privacy",
    "proxy",
    "notifications",
    "scripting",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestAuthProvider",
    "management",
    "alarms",
    "offscreen"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "en",
  "declarative_net_request": {
